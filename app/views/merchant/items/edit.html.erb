<h2>Edit Item</h2>

<%= form_for @item, url: "/merchant/items/#{@item.id}", method: :put do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br>
<%= f.label :description %>
<%= f.text_field :description%><br>
<%= f.label :price %>
<%= f.text_field :price%><br>
<%= f.label :image %>
<%= f.text_field :image%><br>
<%= f.label :inventory %>
<%= f.text_field :inventory%><br>
<%= f.submit %>
<% end %>
